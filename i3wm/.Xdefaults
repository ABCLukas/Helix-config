Xft.dpi:        	96
Xft.antialias:  	true
Xft.rgba:       	rgb
Xft.hinting:    	true
Xft.hintstyle: 		hintfull
*foreground:     	#E5E9F0
*background:     	#2E3440 
!black
*color0:          	rgb:19/19/19 
*color8:          	rgb:25/25/25 
!red 
*color1:         	rgb:80/32/32 
*color9:         	rgb:98/2b/2b 
!green 
*color2:         	rgb:5b/76/2f 
*color10:        	rgb:89/b8/3f 
!brown/yellow 
*color3:         	rgb:aa/99/43 
*color11:        	rgb:ef/ef/60 
!blue 
*color4:         	rgb:32/4c/80 
*color12:        	rgb:2b/4f/98
!magenta 
*color5:         	rgb:70/6c/9a 
*color13:        	rgb:82/6a/b1 
!cyan 
*color6:         	rgb:92/b1/9e 
*color14:        	rgb:a1/cd/cd 
!white 
*color7:         	#E5E9F0 
*color15:        	#8FBCBB  

URxvt.inheritPixmap: 	true 
URxvt.transparent: 	true 
URxvt.tintColor: 	#fff
URxvt.shading: 		45
URxvt.scrollBar:	False 
URxvt.saveLines:	3000
!Browser Settings
URxvt.preeditType:	Root
URxvt.urlLauncher: 	firefox
URxvt.matcher.button: 	1
URxvt.perl-ext-common: 	matcher
!Fonts
URxvt.letterSpace: 	-1
URxvt.font: 		9x15
URxvt*geometry:		80x30
URxvt.font:		xft:bitstream Vera Sans Mono:bold:size=9
